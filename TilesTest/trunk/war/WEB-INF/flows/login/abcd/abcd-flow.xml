<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://www.springframework.org/schema/webflow
                          http://www.springframework.org/schema/webflow/spring-webflow-1.0.xsd">

	<start-state idref="a"/>
	
	<view-state id="a" view="login/abcd/a">
		<transition on="submitB" to="b"/>
		<transition on="submitC" to="c"/>
	</view-state>
	
	<view-state id="b" view="login/abcd/b">
		<transition on="submitC" to="c"/>
		<transition on="submitD" to="d"/>
	</view-state>
	
	<view-state id="c" view="login/abcd/c">
		<transition on="submitB" to="b"/>
		<transition on="submitD" to="d"/>
	</view-state>
	
	
	<end-state id="d" view="login/abcd/d" />
	
</flow>